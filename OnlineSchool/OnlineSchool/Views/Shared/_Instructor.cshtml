<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

    <link href="~/Content/Instructor/bootstrap.min.css" rel="stylesheet" />
    @*<link href="~/Content/bower_components/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />*@
    <link href="~/Content/bower_components/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    @*<link href="~/Content/bower_components/Ionicons/css/ionicons.min.css" rel="stylesheet" />*@

    
    @*<link href="~/Content/Instructor/font-awesome.min.css" rel="stylesheet" />*@
    <link href="~/Content/Instructor/ionicons.min.css" rel="stylesheet" />

    <link href="~/Content/bower_components/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet" />
    <link href="~/Content/bower_components/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css" rel="stylesheet" />
    <link href="~/Content/plugins/iCheck/all.css" rel="stylesheet" />

    @*<link href="~/Content/Instructor/daterangepicker.css" rel="stylesheet" />
        <link href="~/Content/Instructor/bootstrap-datepicker.min.css" rel="stylesheet" />
        <link href="~/Content/Instructor/all.css" rel="stylesheet" />*@

    <link href="~/Content/bower_components/bootstrap-colorpicker/dist/css/bootstrap-colorpicker.min.css" rel="stylesheet" />
    <link href="~/Content/plugins/timepicker/bootstrap-timepicker.min.css" rel="stylesheet" />
    @*<link href="~/Content/Instructor/bootstrap-colorpicker.min.css" rel="stylesheet" />
        <link href="~/Content/Instructor/bootstrap-timepicker.min.css" rel="stylesheet" />*@

    <link href="~/Content/bower_components/select2/dist/css/select2.min.css" rel="stylesheet" />
    @*<link href="~/Content/dist/css/AdminLTE.min.css" rel="stylesheet" />*@
    <link href="~/Content/dist/css/skins/_all-skins.min.css" rel="stylesheet" />

    @*<link href="~/Content/Instructor/select2.min.css" rel="stylesheet" />*@
    <link href="~/Content/Instructor/AdminLTE.min.css" rel="stylesheet" />
    @*<link href="~/Content/Instructor/_all-skins.min.css" rel="stylesheet" />*@

    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    @RenderSection("styles", false)
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">

        <header class="main-header">
            <!-- Logo -->
            <a href="../index2.html" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>A</b>LT</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>Schoo</b>ly</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>

                <div class="navbar-custom-menu">
                    @if (Request.IsAuthenticated)
                    {
                        using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
                        {
                            @Html.AntiForgeryToken()

                            <ul class="nav navbar-nav ">

                                <li><a> Hello @User.Identity.Name</a></li>

                                <li><a href="javascript:document.getElementById('logoutForm').submit()">Logoff</a></li>
                            </ul>
                        }
                    }
                    <ul class="nav navbar-nav">
                        
                        <li class="dropdown user user-menu">
                            @*<a href="#" class="dropdown-toggle" data-toggle="dropdown">*@
                               
                           @* </a>*@
                            <ul class="dropdown-menu">
                                <!-- User image -->
                               
                                <li class="user-footer">

                                    <div class="pull-right">
                                        <a href="#" class="btn btn-default btn-flat">Sign out</a>
                                    </div>
                                </li>
                            </ul>
                        </li>

                    </ul>
                </div>
            </nav>
        </header>
        



        @RenderBody()
        <div class="control-sidebar-bg"></div>
    </div>
    <script src="~/Content/Instructor/Js/jquery.min.js"></script>
    <script src="~/Content/Instructor/Js/bootstrap.min.js"></script>
    <script src="~/Content/Instructor/Js/jquery.slimscroll.min.js"></script>
    <script src="~/Content/Instructor/Js/fastclick.js"></script>

    <script src="~/Content/Instructor/Js/adminlte.min.js"></script>
    <script src="~/Content/Instructor/Js/demo.js"></script>
    @RenderSection("scripts", required: false)
    
</body>
</html>







        

    
 